[
  {
    "key": "ctrl+alt+d",
    "command": "workbench.action.tasks.runTask",
    "args": "Minikube: Dashboard (Open)",
    "when": "editorTextFocus || terminalFocus"
  },
  {
    "key": "ctrl+alt+u",
    "command": "workbench.action.tasks.runTask",
    "args": "Minikube: Dashboard URL",
    "when": "editorTextFocus || terminalFocus"
  },
  {
    "key": "ctrl+alt+x",
    "command": "workbench.action.tasks.runTask",
    "args": "Minikube: Dashboard Stop",
    "when": "editorTextFocus || terminalFocus"
  }
]
